<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>it.unibz.inf.ontouml</groupId>
	<artifactId>it.unibz.inf.ontouml.vp</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<name>OntoUML-2.0-for-Visual-Paradigm</name>
	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<repo.dir>${project.basedir}/repo</repo.dir>
		<classes.dir>${project.basedir}/classes</classes.dir>
		<!-- <visualparadigm.app.dir>/Applications/Visual Paradigm.app/Contents/Resources/app</visualparadigm.app.dir> -->
	</properties>
	<dependencies>
		<dependency>
			<groupId>com.google.code.gson</groupId>
			<artifactId>gson</artifactId>
			<version>2.8.5</version>
		</dependency>
		<dependency>
			<groupId>com.visualparadigm.app</groupId>
			<artifactId>com.visualparadigm.app.lib.openapi</artifactId>
			<version>1.0.0</version>
		</dependency>
		<dependency>
			<groupId>com.visualparadigm.app</groupId>
			<artifactId>com.visualparadigm.app.lib.vpplatform</artifactId>
			<version>1.0.0</version>
		</dependency>
		<dependency>
			<groupId>com.visualparadigm.app</groupId>
			<artifactId>com.visualparadigm.app.lib.lib01</artifactId>
			<version>1.0.0</version>
		</dependency>
		<dependency>
			<groupId>com.visualparadigm.app</groupId>
			<artifactId>com.visualparadigm.app.lib.lib02</artifactId>
			<version>1.0.0</version>
		</dependency>
		<dependency>
			<groupId>com.visualparadigm.app</groupId>
			<artifactId>com.visualparadigm.app.lib.lib03</artifactId>
			<version>1.0.0</version>
		</dependency>
		<dependency>
			<groupId>com.visualparadigm.app</groupId>
			<artifactId>com.visualparadigm.app.lib.lib04</artifactId>
			<version>1.0.0</version>
		</dependency>
		<dependency>
			<groupId>com.visualparadigm.app</groupId>
			<artifactId>com.visualparadigm.app.lib.lib05</artifactId>
			<version>1.0.0</version>
		</dependency>
		<dependency>
			<groupId>com.visualparadigm.app</groupId>
			<artifactId>com.visualparadigm.app.lib.lib06</artifactId>
			<version>1.0.0</version>
		</dependency>
		<dependency>
			<groupId>com.visualparadigm.app</groupId>
			<artifactId>com.visualparadigm.app.lib.lib07</artifactId>
			<version>1.0.0</version>
		</dependency>
		<dependency>
			<groupId>com.visualparadigm.app</groupId>
			<artifactId>com.visualparadigm.app.lib.lib09</artifactId>
			<version>1.0.0</version>
		</dependency>
		<dependency>
			<groupId>com.visualparadigm.app</groupId>
			<artifactId>com.visualparadigm.app.lib.jniwrap</artifactId>
			<version>1.0.0</version>
		</dependency>
		<dependency>
			<groupId>com.visualparadigm.app</groupId>
			<artifactId>com.visualparadigm.app.lib.winpack</artifactId>
			<version>1.0.0</version>
		</dependency>
		<dependency>
			<groupId>com.visualparadigm.app</groupId>
			<artifactId>com.visualparadigm.app.ormlib.orm</artifactId>
			<version>1.0.0</version>
		</dependency>
	</dependencies>
	<build>
		<outputDirectory>${classes.dir}</outputDirectory>
		<pluginManagement>
			<plugins>
				<!--This plugin's configuration is used to store Eclipse m2e settings 
					only. It has no influence on the Maven build itself. -->
				<plugin>
					<groupId>org.eclipse.m2e</groupId>
					<artifactId>lifecycle-mapping</artifactId>
					<version>1.0.0</version>
					<configuration>
						<lifecycleMappingMetadata>
							<pluginExecutions>
								<pluginExecution>
									<pluginExecutionFilter>
										<groupId>org.apache.maven.plugins</groupId>
										<artifactId>maven-install-plugin</artifactId>
										<versionRange>[2.5.2,)</versionRange>
										<goals>
											<goal>install-file</goal>
										</goals>
									</pluginExecutionFilter>
									<action>
										<execute />
									</action>
								</pluginExecution>
							</pluginExecutions>
						</lifecycleMappingMetadata>
					</configuration>
				</plugin>
				<plugin>
					<artifactId>maven-clean-plugin</artifactId>
					<version>3.1.0</version>
					<executions>
						<execution>
							<id>clean-plugin</id>
							<phase>clean</phase>
							<goals>
								<goal>clean</goal>
							</goals>
						</execution>
					</executions>
				</plugin>
				<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-install-plugin</artifactId>
					<version>2.5.2</version>
					<executions>
						<execution>
							<id>openapi</id>
							<phase>initialize</phase>
							<goals>
								<goal>install-file</goal>
							</goals>
							<configuration>
								<groupId>com.visualparadigm.app</groupId>
								<artifactId>com.visualparadigm.app.lib.openapi</artifactId>
								<version>1.0.0</version>
								<packaging>jar</packaging>
								<file>${visualparadigm.app.dir}/lib/openapi.jar</file>
							</configuration>
						</execution>
						<execution>
							<id>vpplatform</id>
							<phase>initialize</phase>
							<goals>
								<goal>install-file</goal>
							</goals>
							<configuration>
								<groupId>com.visualparadigm.app</groupId>
								<artifactId>com.visualparadigm.app.lib.vpplatform</artifactId>
								<version>1.0.0</version>
								<packaging>jar</packaging>
								<file>${visualparadigm.app.dir}/lib/vpplatform.jar</file>
							</configuration>
						</execution>
						<execution>
							<id>lib01</id>
							<phase>initialize</phase>
							<goals>
								<goal>install-file</goal>
							</goals>
							<configuration>
								<groupId>com.visualparadigm.app</groupId>
								<artifactId>com.visualparadigm.app.lib.lib01</artifactId>
								<version>1.0.0</version>
								<packaging>jar</packaging>
								<file>${visualparadigm.app.dir}/lib/lib01.jar</file>
							</configuration>
						</execution>
						<execution>
							<id>lib02</id>
							<phase>initialize</phase>
							<goals>
								<goal>install-file</goal>
							</goals>
							<configuration>
								<groupId>com.visualparadigm.app</groupId>
								<artifactId>com.visualparadigm.app.lib.lib02</artifactId>
								<version>1.0.0</version>
								<packaging>jar</packaging>
								<file>${visualparadigm.app.dir}/lib/lib02.jar</file>
							</configuration>
						</execution>
						<execution>
							<id>lib03</id>
							<phase>initialize</phase>
							<goals>
								<goal>install-file</goal>
							</goals>
							<configuration>
								<groupId>com.visualparadigm.app</groupId>
								<artifactId>com.visualparadigm.app.lib.lib03</artifactId>
								<version>1.0.0</version>
								<packaging>jar</packaging>
								<file>${visualparadigm.app.dir}/lib/lib03.jar</file>
							</configuration>
						</execution>
						<execution>
							<id>lib04</id>
							<phase>initialize</phase>
							<goals>
								<goal>install-file</goal>
							</goals>
							<configuration>
								<groupId>com.visualparadigm.app</groupId>
								<artifactId>com.visualparadigm.app.lib.lib04</artifactId>
								<version>1.0.0</version>
								<packaging>jar</packaging>
								<file>${visualparadigm.app.dir}/lib/lib04.jar</file>
							</configuration>
						</execution>
						<execution>
							<id>lib05</id>
							<phase>initialize</phase>
							<goals>
								<goal>install-file</goal>
							</goals>
							<configuration>
								<groupId>com.visualparadigm.app</groupId>
								<artifactId>com.visualparadigm.app.lib.lib05</artifactId>
								<version>1.0.0</version>
								<packaging>jar</packaging>
								<file>${visualparadigm.app.dir}/lib/lib05.jar</file>
							</configuration>
						</execution>
						<execution>
							<id>lib06</id>
							<phase>initialize</phase>
							<goals>
								<goal>install-file</goal>
							</goals>
							<configuration>
								<groupId>com.visualparadigm.app</groupId>
								<artifactId>com.visualparadigm.app.lib.lib06</artifactId>
								<version>1.0.0</version>
								<packaging>jar</packaging>
								<file>${visualparadigm.app.dir}/lib/lib06.jar</file>
							</configuration>
						</execution>
						<execution>
							<id>lib07</id>
							<phase>initialize</phase>
							<goals>
								<goal>install-file</goal>
							</goals>
							<configuration>
								<groupId>com.visualparadigm.app</groupId>
								<artifactId>com.visualparadigm.app.lib.lib07</artifactId>
								<version>1.0.0</version>
								<packaging>jar</packaging>
								<file>${visualparadigm.app.dir}/lib/lib07.jar</file>
							</configuration>
						</execution>
						<execution>
							<id>lib09</id>
							<phase>initialize</phase>
							<goals>
								<goal>install-file</goal>
							</goals>
							<configuration>
								<groupId>com.visualparadigm.app</groupId>
								<artifactId>com.visualparadigm.app.lib.lib09</artifactId>
								<version>1.0.0</version>
								<packaging>jar</packaging>
								<file>${visualparadigm.app.dir}/lib/lib09.jar</file>
							</configuration>
						</execution>
						<execution>
							<id>jniwrap</id>
							<phase>initialize</phase>
							<goals>
								<goal>install-file</goal>
							</goals>
							<configuration>
								<groupId>com.visualparadigm.app</groupId>
								<artifactId>com.visualparadigm.app.lib.jniwrap</artifactId>
								<version>1.0.0</version>
								<packaging>jar</packaging>
								<file>${visualparadigm.app.dir}/lib/jniwrap.jar</file>
							</configuration>
						</execution>
						<execution>
							<id>winpack</id>
							<phase>initialize</phase>
							<goals>
								<goal>install-file</goal>
							</goals>
							<configuration>
								<groupId>com.visualparadigm.app</groupId>
								<artifactId>com.visualparadigm.app.lib.winpack</artifactId>
								<version>1.0.0</version>
								<packaging>jar</packaging>
								<file>${visualparadigm.app.dir}/lib/winpack.jar</file>
							</configuration>
						</execution>
						<execution>
							<id>orm</id>
							<phase>initialize</phase>
							<goals>
								<goal>install-file</goal>
							</goals>
							<configuration>
								<groupId>com.visualparadigm.app</groupId>
								<artifactId>com.visualparadigm.app.ormlib.orm</artifactId>
								<version>1.0.0</version>
								<packaging>jar</packaging>
								<file>${visualparadigm.app.dir}/ormlib/orm.jar</file>
							</configuration>
						</execution>
					</executions>
				</plugin>
				<plugin>
					<executions>
						<execution>
							<id>compiler-plugin</id>
							<phase>compile</phase>
							<goals>
								<goal>compile</goal>
							</goals>
						</execution>
					</executions>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-compiler-plugin</artifactId>
					<version>3.8.1</version>
					<configuration>
						<source>1.8</source>
						<target>1.8</target>
					</configuration>
				</plugin>
				<plugin>
			        <groupId>org.codehaus.mojo</groupId>
			        <artifactId>properties-maven-plugin</artifactId>
			        <version>1.0.0</version>
			        <executions>
			          <execution>
			            <phase>initialize</phase>
			            <goals>
			              <goal>read-project-properties</goal>
			            </goals>
			            <configuration>
			              <files>
			                <file>env.properties</file>
			              </files>
			            </configuration>
			          </execution>
			        </executions>
		      	</plugin>
			</plugins>
		</pluginManagement>
	</build>
</project>